{
	"name": "gilbertsoft/gswarranty",
	"type": "typo3-cms-extension",
	"description": "GS Warranty adapts the TYPO3 backend logos and warranty data and registers a new toolbar item. Feel free to use this extension as base for your own work.",
	"homepage": "https://github.com/gilbertsoft/typo3-gswarranty",
	"license": ["GPL-3.0+"],
	"keywords": ["TYPO3 CMS", "Gilbertsoft"],
	"support": {
		"issues": "https://github.com/gilbertsoft/typo3-gswarranty/issues"
	},
	"require": {
		"typo3/cms-core": "^7.6.0 || ^8.7.0",
		"typo3/cms-backend": "^7.6.0 || ^8.7.0",
		"typo3/cms-t3skin": "^7.6.0 || ^8.7.0"
	},
	"autoload": {
		"psr-4": {
			"Gilbertsoft\\Warranty\\": "Classes"
		}
	},
	"replace": {
		"gswarranty": "self.version",
		"typo3-ter/gswarranty": "self.version"
	}
}
